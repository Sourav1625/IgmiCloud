<Application x:Class="IGMICloudApplication.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    DispatcherUnhandledException="APP_DispatcherUnhandledException"
    Startup="OnStartup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/ToastNotifications.Messages;component/Themes/Default.xaml" />
                <ResourceDictionary Source="Views/Styles/CoreResourceDictionary.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Views/ModernButtonTheme/ModernButton.xaml"/>
                <ResourceDictionary Source="Views/Styles/Blue.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Views/Styles/CoreResourceDictionary.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Views/Styles/DataWindowResourceDictionary.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Views/Styles/UserControlResourceDictionary.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>
    </Application.Resources>
</Application>
